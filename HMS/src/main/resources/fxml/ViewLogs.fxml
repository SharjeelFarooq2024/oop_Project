<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.*?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.myapp.frontend.controllers.ViewLogsController"
      alignment="CENTER" spacing="20" style="-fx-background-color: white; -fx-padding: 30;">
    <Label text="System Logs" style="-fx-font-size: 22px; -fx-font-weight: bold;"/>
    <TextArea fx:id="logsTextArea" editable="false" wrapText="true" prefWidth="600" prefHeight="400"/>
    <Button fx:id="backButton" text="Back to Dashboard" style="-fx-background-color: #3498db; -fx-text-fill: white;"/>
</VBox>
